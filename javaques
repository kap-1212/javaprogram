package kapilmalik;

public class Javaques {
	 public static void main(String[ ] args)
	  {
	  Scanner scan = new Scanner(System.in);
	 int a[ ], p[ ], i;
	  a = new int[5];
	  p = new int[5];
	System.out.println("Enter Nuember:");
	           for(i=0;i<5;i++)
	   {
	     a[i]=scan.nextint();
	   }
	    for(i=0;i<5;i++)
	  {
	   a[i]=a[i]*a[i];
	}
	  for(i=0;i<5;i++)
	{
	 System.out.print(a[i]+" ");
	   }
	  }
